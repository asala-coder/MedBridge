{
  "openapi": "3.0.1",
  "info": {
    "title": "MedBridge",
    "version": "1.0"
  },
  "paths": {
    "/api/cart/add": {
      "post": {
        "tags": [
          "Cart"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddToCartDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AddToCartDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AddToCartDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/MedBridge/signup": {
